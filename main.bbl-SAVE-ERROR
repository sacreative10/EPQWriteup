      \strng{namehash}{a7427cbe3b1cd74a79b2c533ea17a5ff}
      \strng{fullhash}{a7427cbe3b1cd74a79b2c533ea17a5ff}
      \strng{fullhashraw}{a7427cbe3b1cd74a79b2c533ea17a5ff}
      \strng{bibnamehash}{a7427cbe3b1cd74a79b2c533ea17a5ff}
      \strng{authorbibnamehash}{a7427cbe3b1cd74a79b2c533ea17a5ff}
      \strng{authornamehash}{a7427cbe3b1cd74a79b2c533ea17a5ff}
      \strng{authorfullhash}{a7427cbe3b1cd74a79b2c533ea17a5ff}
      \strng{authorfullhashraw}{a7427cbe3b1cd74a79b2c533ea17a5ff}
