    \entry{reynolds_flocks_1987}{inproceedings}{}{}
      \name{author}{1}{}{%
        {{hash=c565938772b0a9629e74a8f8f657693b}{%
           family={Reynolds},
           familyi={R\bibinitperiod},
           given={Craig\bibnamedelima W.},
           giveni={C\bibinitperiod\bibinitdelim W\bibinitperiod}}}%
      }
      \list{location}{1}{%
